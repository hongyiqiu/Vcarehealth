msgid "Open Panel On"
msgstr ""

#: includes/class-accordion-slider-settings.php:242
msgid "Hover"
msgstr ""

#: includes/class-accordion-slider-settings.php:243
msgid "Click"
msgstr ""

#: includes/class-accordion-slider-settings.php:244
msgid "Never"
msgstr ""

#: includes/class-accordion-slider-settings.php:246
msgid ""
